{
  "name": "[module_template_name]",
  "context": ".",

  "defaultProfile": "default_server",

  "profiles": {

    "default_server": {
      "host": "[server_ip]",
      "protocol": "sftp",
      "port": 22,
      "username": "[server_user]",
      "password": "[server_password]",
      "interactiveAuth": false,
      "secure": false,
      "remotePath": "[remote_path]"
    },

    "server_ssh": {
      "host": "[server_ssh_ip]",
      "protocol": "sftp",
      "port": 22,
      "username": "[ssh_user]",
      "agent": "ssh-agent",
      "privateKeyPath": "[local_path_to_id_rsa_file]",
      "passphrase": "[password_ssh_file]",
      "interactiveAuth": false,
      "secure": false,
      "remotePath": "[remote_path]"
    }

  },

  "ignore": [
    ".git",
    ".git/**",
    ".github",
    ".github/**",
    ".gitlab",
    ".gitlab/**",
    ".gitlab-ci.yml",
    ".gitignore",
    ".gitattributes",
    ".tmp-doc",
    ".tmp-doc/**",
    ".vscode",
    ".vscode/**",
    ".versionrc",
    ".wakatime-project",
    ".well-known",
    "documentation",
    "documentation/**",
    "docs/thunder-tests",
    "node_modules",
    "node_modules/**",
    "debug.log",
    "package.json",
    "package-lock.json",
    "phpcs.xml",
    "phpdox.xml",
    "phpmd.xml",
    "docker/msyql",
    "docker/mysql/**",
    "docker/apache-php/ssl",
    "docker/apache-php/ssl/**"
  ],
  "connectTimeout": 10000,
  "uploadOnSave": true,
  "downloadOnOpen": true,
  "syncMode": "full",
  "watcher": {
    "files": "**/*{css/*.map,css/*.css,js/*.js,CHANGELOG.md}",
    "autoUpload": true,
    "autoDelete": true
  },
  "concurrency": 4

}
