{
  "name": "[project_name]",
  "context": ".",

  "defaultProfile": "server_1",

  "profiles": {

    "server_1": {
      "host": "[server_ip]",
      "port": 22,
      "username": "[user]",
      "password": "[password]",
      "protocol": "[sftp or ftp]",
      "interactiveAuth": false,
      "secure": false,
      "remotePath": "[remote_path]",
      "ignore": [
        ".git",
        ".git/**",
        ".github",
        ".github/**",
        "debug.log"
      ],
      "connectTimeout": 10000,
      "uploadOnSave": true,
      "downloadOnOpen": true,
      "syncMode": "full",
      "watcher": {
        "files": "css/*.{map,css}",
        "autoUpload": true,
        "autoDelete": true
      },
      "concurrency": 4
    },

    "server_2": {
      "host": "[server_ip]",
      "port": 22,
      "username": "[user]",
      "password": "[password]",
      "protocol": "[sftp or ftp]",
      "interactiveAuth": false,
      "secure": false,
      "remotePath": "[remote_path]",
      "ignore": [
        ".git",
        ".git/**",
        ".github",
        ".github/**",
        ".vscode",
        ".vscode/**",
        "debug.log"
      ],
      "connectTimeout": 10000,
      "uploadOnSave": true,
      "downloadOnOpen": true,
      "syncMode": "full",
      "watcher": {
        "files": "css/*.{map,css}",
        "autoUpload": true,
        "autoDelete": true
      },
      "concurrency": 4
    }
  }

}
