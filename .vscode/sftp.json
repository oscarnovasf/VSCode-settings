{
  "name": "[project_name]",
  "context": ".",
  "protocol": "[sftp or ftp]",
  "host": "[server_ip]",
  "port": 22,
  "username": "[user]",
  "password": "[password]",
  "interactiveAuth": false,
  "secure": false,
  "remotePath": "[remote_path]",
  "connectTimeout": 10000,
  "uploadOnSave": true,
  "downloadOnOpen": true,
  "syncMode": "full",
  "ignore": [
    ".git",
    ".git/**",
    ".github",
    ".github/**",
    ".vscode",
    ".vscode/**",
    "debug.log"
  ],
  "watcher": {
    "files": "css/*.{map,css}",
    "autoUpload": true,
    "autoDelete": true
  },
  "concurrency": 4
}
