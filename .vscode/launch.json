{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "XDebug [LANDO]",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "log": false,
      "stopOnEntry": false,
      "xdebugSettings": {
        "max_children": 128,
        "max_data": 1024,
        "max_depth": 3,
        "show_hidden": 1
      },
      "pathMappings": {
        "/app/": "${workspaceFolder}/",
      }
    }
  ]
}
